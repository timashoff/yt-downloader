{
  "name": "yt-audio-downloader",
  "version": "1.0.0",
  "description": "CLI tool for downloading audio from YouTube videos",
  "type": "module",
  "main": "src/cli.js",
  "bin": {
    "yt-audio": "./bin/yt-audio"
  },
  "scripts": {
    "start": "node src/cli.js",
    "check-dead-code": "npx unimported",
    "lint": "npx eslint src/ --fix",
    "update-ytdlp": "YOUTUBE_DL_SKIP_DOWNLOAD= npm install --force"
  },
  "keywords": [
    "youtube",
    "audio",
    "download",
    "cli",
    "yt-dlp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "youtube-dl-exec": "^3.0.22",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
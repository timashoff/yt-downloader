{
  "name": "universal-downloader",
  "version": "1.0.0",
  "description": "Universal CLI downloader for audio and video from any website",
  "type": "module",
  "main": "src/cli.js",
  "bin": {
    "udav": "./bin/udav"
  },
  "scripts": {
    "start": "node src/cli.js",
    "audio": "node src/cli.js -a",
    "video": "node src/cli.js -v",
    "check-dead-code": "npx unimported",
    "lint": "npx eslint src/ --fix",
    "update-ytdlp": "YOUTUBE_DL_SKIP_DOWNLOAD= npm install --force"
  },
  "keywords": [
    "downloader",
    "universal",
    "audio",
    "video",
    "cli",
    "yt-dlp",
    "youtube",
    "udav"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "youtube-dl-exec": "^3.0.22",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}